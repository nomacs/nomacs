msgstr "míľ za hodinu"

#: src/tags.cpp:1903
msgid "knots"
msgstr "uzlov"

#: src/tags.cpp:1908
msgid "True direction"
msgstr "Skutočný smer"

#: src/tags.cpp:1909
msgid "Magnetic direction"
msgstr "Magnetický smer"

#: src/tags.cpp:1914
msgid "Kilometers"
msgstr "Kilometrov"

#: src/tags.cpp:1915
